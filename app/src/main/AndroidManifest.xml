<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.will.ripperview_demo">

    <application
        android:name=".AppT"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RippleActivity" android:label="RippleActivity"/>
        <activity android:name=".PathMeasureAnimatorActivity" android:label="PathMeasureAnimatorActivity"/>
        <activity android:name=".MarkAndForkActivity" android:label="MarkAndForkActivity"/>
        <activity android:name=".UIRelativeLayoutActivity" android:label="UIRelativeLayoutActivity"/>
        <activity android:name=".ParallaxActivity" android:label="ParallaxActivity"/>
        <activity android:name=".ScreenLayoutActivity" android:label="ScreenLayoutActivity"/>
        <activity android:name=".PercentActivity" android:label="PercentActivity"/>
        <activity android:name=".CustomizePercentActivity" android:label="CustomizePercentActivity"/>
        <activity android:name=".DensityActivity" android:label="DensityActivity"/>
        <activity android:name=".DisplayCutoutActivity" android:label="DisplayCutoutActivity"/>
        <activity android:name=".MusicActivity" android:label="MusicActivity"/>
    </application>

</manifest>